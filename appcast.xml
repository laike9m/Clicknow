            <sparkle:version>1.13.5</sparkle:version>
            <sparkle:shortVersionString>1.13.5</sparkle:shortVersionString>
            <sparkle:minimumSystemVersion>12.0</sparkle:minimumSystemVersion>
            <enclosure url="https://github.com/laike9m/Clicknow/releases/download/1.13.5/Clicknow-1.13.5.dmg" length="6155274" type="application/octet-stream" sparkle:edSignature="nbVEVZBb6J61BL9W2wDUaIJM6lF471qDNBclKnu9DP48yNdHRGeLU2/zcixyZlPNoqw+5+mfWxpFhbg9vdG+DA=="/>
            <description><![CDATA[
              <h3>Improvements</h3>
              <ul>
              <li>Settings window is now placed in the center of the screen.</li>
              <li>You can now use spell checker in your browser's address bar.</li>
              </ul>
              
              <h3>Fixed Bugs</h3>
              <ul>
              <li>Dictionary sometimes shows examples in the wrong language.</li>
              <li>Spell checker is not usable when keyboard shortcut is set.</li>
              </ul>
            ]]></description>
